  <div ng-controller="NavController as ctrl" ng-show="ctrl.isLoggedIn()">
  <md-fab-speed-dial id="btnMenu" md-open="ctrl.isOpen" ng-mouseenter="ctrl.isOpen = true"ng-mouseleave="ctrl.isOpen = false"  md-direction="left" class="md-scale md-fab-bottom-right">
    <md-fab-trigger>
      <md-button aria-label="menu" class="md-fab md-warn">
        <md-icon>menu</md-icon>
      </md-button>
    </md-fab-trigger>

    <md-fab-actions>
      <md-button aria-label="Twitter" class="md-fab md-raised md-mini" ng-click="ctrl.logOut()">
        <md-icon aria-label="Sign Out">power_settings_new</md-icon>
      </md-button>
    </md-fab-actions>
  </md-fab-speed-dial>
